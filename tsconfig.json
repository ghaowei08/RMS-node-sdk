    "module": "commonjs",
    "rootDir": "./",
    "moduleResolution": "node",
    "declaration": true,
    "outDir": "./npm/dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": false
  },
  "exclude": [
    "example"
  ]
}